{% paginate blog.articles by 3 %}
    {%  for article in blog.articles %}
        <div>
            {% if article.image %}
                <img src="{{  article.image | image_url: 'large' }}" alt="{{ article.image.alt }}" width="500" height="500">
                {% else %}
                    {{ 'image' | placeholder_svg_tag: 'w-20' }}
                
            {% endif %}
        </div>

    {%  endfor %}
{%  endpaginate  %}